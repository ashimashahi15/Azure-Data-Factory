				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"folderPath": "Address_Excute_pipeline",
						"fileSystem": "landing"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": []
		},
