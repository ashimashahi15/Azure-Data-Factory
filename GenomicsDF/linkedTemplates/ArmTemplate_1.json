			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GenomicsDF"
		},
		"gen1_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'gen1_ls'"
		},
		"humanTissue_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'humanTissue_ls'"
		},
		"human_tissue_df_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'human_tissue_df_ls'"
